<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.study.heguodong">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".MainActivity$MyBroadcastReceiver">
            <intent-filter>
                <action android:name="com.haier.uhomecontrol.hood.action.ACTION_OPEN_JNI_COMPLETE"/>
                <action android:name="com.haier.uhomecontrol.hood.action.ACTION_CLOSE_JNI_COMPLETE"/>

            </intent-filter>

        </receiver>
    </application>

</manifest>